<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Skillbox_hw_quiz" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#1440C3</item>
        <item name="colorPrimaryVariant">#4667DF</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#CD1AFF</item>
        <item name="colorSecondaryVariant">#CC6FFF</item>
        <item name="colorOnSecondary">#000000</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#1F7A7A</item>
        <!-- Customize your theme here. -->
        <item name="fontFamily">@font/opensans_regular</item>
    </style>

    <style name="Theme.Skillbox_hw_quiz.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Skillbox_hw_quiz.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Skillbox_hw_quiz.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
